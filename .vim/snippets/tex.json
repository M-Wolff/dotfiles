{
  "letter template": {
    "prefix": "letter",
    "body": [
      "b",
      "%!TeX ts-program = xelatex",
      "%!TeX encoding = utf-8 Unicode",
      "\\documentclass[a4paper,11pt]{letter}",
      "\\usepackage[margin=1.25in]{geometry}",
      "\\usepackage[proportional,osf,scaled=1.064]{erewhon}",
      "\\usepackage{pdfpages}",
      "%\\date{August 12, 2016}",
      "\\address{",
      "    ${1:Address line 1} \\\\\\\\ % Sender's address",
      "    ${2:Address line 2} \\\\\\\\ ",
      "    ${3:Address line 3}",
      "}",
      "\\signature{",
      "    ${4:Author} \\\\\\\\ ",
      "    ${5:Email address} \\\\\\\\ ",
      "    ${6:Phone number}",
      "}",
      "\\begin{document}",
      "\\begin{letter}",
      "{",
      "    ${7:Recipient} \\\\\\\\",
      "    ${8:Address line 1} \\\\\\\\",
      "    ${9:Address line 2} \\\\\\\\",
      "    ${10:Address line 3}",
      "}",
      "\\opening{Dear Recipient,}",
      "",
      "Blah.",
      "",
      "\\closing{Sincerely,}",
      "",
      "\\ps{\\textsc{p.s.} Please find a document attached.} % Postscript",
      "\\end{letter}",
      "%\\includepdf[pages={-},scale=0.85]{document.pdf} % attachments",
      "\\end{document}"
    ],
    "description": "letter template"
  },
  "note template": {
    "prefix": "note",
    "body": [
      "b",
      "%!TeX ts-program = xelatex",
      "%!TeX encoding = utf-8 Unicode",
      "\\documentclass[11pt,a4paper]{article}",
      "",
      "\\title{${1:Title}}",
      "\\author{${2:Author}}",
      "",
      "\\begin{document}",
      "",
      "\\maketitle",
      "",
      "\\end{document}"
    ],
    "description": "note template"
  },
  "Table of contents, tables and figures": {
    "prefix": "toc",
    "body": [
      "b",
      "\\tableofcontents",
      "",
      "\\listoffigures",
      "",
      "\\listoftables",
      "",
      "\\pagebreak"
    ],
    "description": "Table of contents, tables and figures"
  },
  "Abstract": {
    "prefix": "abs",
    "body": [
      "b",
      "\\begin{abstract}",
      "",
      "$0",
      "",
      "\\end{abstract}"
    ],
    "description": "Abstract"
  },
  "Summary block": {
    "prefix": "sum",
    "body": [
      "b",
      "\\phantomsection\\addcontentsline{toc}{section}{Summary}",
      "\\section*{Summary} % (fold)",
      "",
      "$0",
      "",
      "% section summary (end)"
    ],
    "description": "Summary block"
  },
  "begin and block": {
    "prefix": "beg",
    "body": [
      "b",
      "\\begin{$1}",
      "",
      "$0",
      "",
      "\\end{$1}"
    ],
    "description": "begin and block"
  },
  "section{} and label{}": {
    "prefix": "sec",
    "body": [
      "b",
      "\\section{${1:SecName}} % (fold)",
      "\\label{sec:${2:${1/\\\\\\w+\\{(.*?)\\}|\\\\(.)|(\\w+)|([^\\w\\\\]+)/(?4:_:\\l$1$2$3)/g}}}",
      "",
      "$0",
      "",
      "% section $2 (end)"
    ],
    "description": "section{} and label{}"
  },
  "subsection{} and label": {
    "prefix": "sub",
    "body": [
      "b",
      "\\subsection{${1:SubName}} % (fold)",
      "\\label{sub:${2:${1/\\\\\\w+\\{(.*?)\\}|\\\\(.)|(\\w+)|([^\\w\\\\]+)/(?4:_:\\l$1$2$3)/g}}}",
      "",
      "$0",
      "",
      "% subsection $2 (end)"
    ],
    "description": "subsection{} and label"
  },
  "subsubsection{} and label": {
    "prefix": "subs",
    "body": [
      "b",
      "\\subsubsection{${1:SubsubName}} % (fold)",
      "\\label{ssub:${2:${1/\\\\\\w+\\{(.*?)\\}|\\\\(.)|(\\w+)|([^\\w\\\\]+)/(?4:_:\\l$1$2$3)/g}}}",
      "",
      "$0",
      "",
      "% subsubsection $2 (end)"
    ],
    "description": "subsubsection{} and label"
  },
  "Enumerate list": {
    "prefix": "enum",
    "body": [
      "b",
      "\\begin{enumerate}",
      "  \\item $1",
      "\\end{enumerate}"
    ],
    "description": "Enumerate list"
  },
  "Itemised list": {
    "prefix": "itm",
    "body": [
      "b",
      "\\begin{itemize}",
      "  \\item $1",
      "\\end{itemize}"
    ],
    "description": "Itemised list"
  },
  "Item line": {
    "prefix": "it",
    "body": [
      "i",
      "\\item $1"
    ],
    "description": "Item line"
  },
  "Description": {
    "prefix": "des",
    "body": [
      "b",
      "\\begin{description}",
      "  \\item[$1] $0",
      "\\end{description}"
    ],
    "description": "Description"
  },
  "figure and label": {
    "prefix": "fig",
    "body": [
      "b",
      "\\begin{figure}[H]",
      "  \\centering",
      "  \\includegraphics[angle=0,width=1.0\\textwidth]{${1:Filename}}",
      "  \\caption{$0}",
      "  \\label{fig:${2:${1/\\\\\\w+\\{(.*?)\\}|\\\\(.)|(\\w+)|([^\\w\\\\]+)/(?4:_:\\l$1$2$3)/g}}}",
      "\\end{figure}"
    ],
    "description": "figure and label"
  },
  "add table": {
    "prefix": "tabl",
    "body": [
      "b",
      "\\begin{table}[H]",
      "\\caption{${1:Description}}",
      "\\label{tab:${2:${1/\\\\\\w+\\{(.*?)\\}|\\\\(.)|(\\w+)|([^\\w\\\\]+)/(?4:_:\\l$1$2$3)/g}}}",
      "\\centering",
      "  \\begin{tabular}{+l^c}",
      "  \\rowstyle{\\itshape}",
      "         Label1 & Label2 \\\\\\\\",
      "  Full spectrum & 0.358 \\\\\\\\",
      "    Narrow band & 0.343 \\\\\\\\",
      "  \\end{tabular}",
      "\\end{table}"
    ],
    "description": "add table"
  },
  "add paragraph": {
    "prefix": "par",
    "body": [
      "b",
      "\\paragraph{${1:paragraph name}} % (fold)",
      "\\label{par:${2:${1/\\\\\\w+\\{(.*?)\\}|\\\\(.)|(\\w+)|([^\\w\\\\]+)/(?4:_:\\l$1$2$3)/g}}}",
      "",
      "$0",
      "",
      "% paragraph $2 (end)"
    ],
    "description": "add paragraph"
  },
  "insert pdf file": {
    "prefix": "pdf",
    "body": [
      "b",
      "\\includepdf[scale=1.0,angle=0,pages={-},pagecommand={}]{${1:Filename}.pdf}"
    ],
    "description": "insert pdf file"
  },
  "reference block": {
    "prefix": "ref",
    "body": [
      "b",
      "\\phantomsection\\addcontentsline{toc}{section}{References}",
      "\\begin{thebibliography}{9}",
      "\\bibitem{${1:label}}",
      "  ${2:Author},",
      "  \\emph{${3:Title}}, ",
      "  ${4:Date}.",
      "\\end{thebibliography}"
    ],
    "description": "reference block"
  },
  "bib item": {
    "prefix": "bi",
    "body": [
      "b",
      "\\bibitem{${1:label}}",
      "  ${2:Author},",
      "  \\emph{${3:Title}}, ",
      "  ${4:Date}."
    ],
    "description": "bib item"
  },
  "Appendix block": {
    "prefix": "apdx",
    "body": [
      "b",
      "\\pagebreak",
      "",
      "\\appendix",
      "",
      "$1"
    ],
    "description": "Appendix block"
  },
  "emphasize": {
    "prefix": "em",
    "body": [
      "i",
      "\\emph{$1}$0"
    ],
    "description": "emphasize"
  },
  "bold": {
    "prefix": "bf",
    "body": [
      "i",
      "\\textbf{$1}$0"
    ],
    "description": "bold"
  },
  "smallcaps": {
    "prefix": "sc",
    "body": [
      "i",
      "\\textsc{$1}$0"
    ],
    "description": "smallcaps"
  },
  "^2": {
    "prefix": "sr",
    "body": [
      "i",
      "^2"
    ],
    "description": "^2"
  },
  "^3": {
    "prefix": "cb",
    "body": [
      "i",
      "^3"
    ],
    "description": "^3"
  },
  "complement": {
    "prefix": "compl",
    "body": [
      "i",
      "^{c}"
    ],
    "description": "complement"
  },
  "superscript": {
    "prefix": "td",
    "body": [
      "i",
      "^{$1}$0"
    ],
    "description": "superscript"
  },
  "Fraction": {
    "prefix": "//",
    "body": [
      "iA",
      "\\\\frac{$1}{$2}$0"
    ],
    "description": "Fraction"
  }
}